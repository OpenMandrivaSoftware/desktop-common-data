?package(OpenOffice.org): needs=x11 section="Use office tools" icon="ooo_draw.png" title="Create a draw using OpenOffice.org" command="oodraw" mimetypes="application/vnd.sun.xml.draw,application/vnd.stardivision.draw"

?package(OpenOffice.org): needs=x11 section="Use office tools" icon="ooo_writer.png" title="Write a document using OpenOffice.org" command="oowriter" mimetypes="application/vnd.sun.xml.writer,application/vnd.stardivision.writer,application/msword,application/vnd.ms-word,application/x-doc,text/rtf"

?package(OpenOffice.org): needs=x11 section="Use office tools" icon="ooo_math.png" title="Create formula using OpenOffice.org" command="oomath" mimetypes="application/vnd.stardivision.math,application/vnd.sun.xml.writer.math"


## View, modify or create a picture

# Manipulate pictures
?package(gimp): command="gimp" icon="wilbur.png" section="View, modify, or create graphics" title="Manipulate pictures" needs="x11" mimetypes="image/gif;image/jpeg;image/png;image/tiff;image/x-xbm;image/x-xpm;image/bmp;image/fits;image/pcx;image/pix;image/pnm;"

# Take a screenshot
?package(kdegraphics): needs="kde" kde_filename="ksnapshot" section="View, modify, or create graphics" title="Take a screenshot" icon="ksnapshot.png" command="ksnapshot" kde_command="ksnapshot %i %m" mimetypes="" kde_opt="\\nBinaryPattern=\\nTerminalOptions=\\nPath=\\nTerminal=0\\nDocPath=ksnapshot/index.html\\nKDE-X-StartupNotify=true"

?package(gnome-core): needs="gnome" section="View, modify, or create graphics" title="Take a screenshot" icon="ksnapshot.png" command="gnome-panel-screenshot"

# Image acquisition
?package(kdegraphics): needs="gnome" section="View, modify, or create graphics" title="Acquire images using a scanner" icon="/usr/share/icons/hicolor/48x48/devices/scanner.png" command="kooka" multiple_files="true" accept_url="true"

?package(kdegraphics): needs="kde" kde_filename="kooka" section="View, modify, or create graphics" title="Acquire images using a scanner" icon="scanner.png" command="kooka" kde_command="kooka %i %m %U" multiple_files="true" accept_url="true" kde_opt="\\nTerminalOptions=\\nPath=\\nTerminal=0\\nBinaryPattern=kooka\\nDocPath=kooka/kooka.html"


# Koffice
?package(koffice): needs="x11" kde_filename="karbon" section="View, modify, or create graphics" title="Create a drawing" icon="karbon.png" command="karbon" mimetypes="application/x-karbon,application/illustrator,image/x-eps,application/postscript" kde_opt="\\nX-KDE-Library=libkarbonpart\\nGenericName=Scalable Graphics\\nInitialPreference=5\\nServiceTypes=KOfficePart,KParts/ReadOnlyPart,KParts/ReadWritePart\\nX-KDE-NativeMimeType=application/x-karbon"

?package(sodipodi): needs="gnome" icon="sodipodi.xpm" section="View, modify, or create graphics" title="Create a drawing" command="/usr/bin/sodipodi"

