exec sawmill
--@@--
NAME=BlackBox
ICON=BlackBox.xpm
EXEC=/usr/X11R6/bin/blackbox
DESC=A Light but nice looking window manager
SCRIPT:
/usr/X11R6/bin/bsetroot -solid Navy
exec /usr/X11R6/bin/blackbox
