--@@--
NAME=Xfce
EXEC=/usr/bin/xfwm
DESC=A CDE like window manager
SCRIPT:
[ "x$XFCE_DATA" = "x" ] && XFCE_DATA=/usr/share/xfce
exec /usr/bin/xfwm
